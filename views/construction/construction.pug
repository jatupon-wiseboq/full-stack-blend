.workspace-container
  .workspace-vertical-container
    .workspace-vertical-panel
      include toolbar/toolbar
  .workspace-vertical-container.full-relative-size
    .workspace-vertical-panel
      .workspace-container
        .workspace-horizontal-container
          .workspace-horizontal-panel(id="explorer")
            include explorer/explorer
          .workspace-horizontal-panel.full-relative-size
            .workspace-container
              .workspace-vertical-container
                .workspace-vertical-panel.siteToolset.dataToolset.servicesToolset.componentsToolset.popupsToolset.toolset
                  include area/area
                .workspace-vertical-panel.propertiesToolset.toolset(style="display: none;")
                  include properties/properties
              .workspace-vertical-container.preview-size-picker-container.siteToolset.servicesToolset.componentsToolset.popupsToolset.toolset(style="height: 26px;")
                .workspace-vertical-panel.text-center(style="border-top: solid 1px #c8c8c8;")
                  +CreateReactComponent('FullStackBlend.Components.PreviewSizePicker')
              include footer/footer
          .workspace-horizontal-panel
            include sidebar/sidebar

